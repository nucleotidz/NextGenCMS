@{
    Layout = null;
}

<div class="row">
    <ul class="breadcrumb">
        <li><a ui-sref="#">Administration</a></li>
        <li>Group Management</li>
    </ul>
</div>
<div class="btnPane">
    <div class="row">
        <div class="col-xs-2">
            <h5>Search Group</h5>
        </div>
        <div class="col-xs-4 pull-left">
            <div class="input-group">
                <input type="text" class="form-control" id="search" placeholder="Search">
                <div class="input-group-addon"><span class="glyphicon glyphicon-search" /></div>
            </div>
        </div>
        <div class="col-xs-6">
            <button type="button" class="btn btn-primary btn-sm bluebtn whiteText pull-right" ng-click="vm.addGroup();">Create Group<span class="glyphicon glyphicon-plus"></span></button>
        </div>
    </div>
</div>