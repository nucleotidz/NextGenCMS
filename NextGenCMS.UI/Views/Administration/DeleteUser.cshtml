
@{
    Layout = null;
}

<div class="modal-header">
    <button type="button" class="close" ng-click="no();"><span aria-hidden="true">&times;</span></button>
    <div class="settingicon pull-left"><span class="glyphicon glyphicon-trash"></span></div><h4 class="modal-title">Delete</h4>
</div>
<div class="modal-body">    
    <div class="row">
        <div class="col-xs-12 col-sm-6 col-md-12">
            {{message}}
        </div>
    </div>  
    <div class="modal-footer">
        <button type="button" class="btn btn-danger" ng-click="yes();">Yes</button>
        <button type="button" class="btn btn-default" ng-click="no();">No</button>
    </div>
</div>


